<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <connectionStrings>
  <add name="ExpenseDBConnection"
           connectionString="Provider=Microsoft.ACE.OLEDB.12.0;Data Source=|Datadirectory|\Expense.xlsx;Extended Properties=&quot;Excel 12.0 Xml;HDR=YES&quot;;"
            providerName="System.Data.OleDb"/>
  </connectionStrings>
  <appSettings>
    <add key="SourcePath" value="E:\Google Drive\"/>
    <add key="TargetPath" value="E:\DatabaseBackup\"/>
  </appSettings>
</configuration>